(\"users\", JSON.stringify(dataUser));\n    localStorage.setItem(\"trip\", JSON.stringify(dataTrip));\n  }, []);\n\n  return (\n    <>\n      <Switch>\n        <Route path=\"/\" exact component={HomePage} />\n        <Route path=\"/detail/:id\" exact component={DetailPage} />\n        <Private path=\"/client/profile\" exact component={ProfilePage} />\n        <Private path=\"/client/order\" exact component={OrderPage} />\n        <Private path=\"/client/payment\" exact component={PendingPage} />\n        <Private path=\"/admin\" exact component={TransactionAdm} />\n        <Private path=\"/admin/trip\" exact component={TripAdmin} />\n      </Switch>\n    </>\n  );\n}\n\nexport default App;\n","/home/usermint/Desktop/dewetour/src/reportWebVitals.js",[],"/home/usermint/Desktop/dewetour/src/pages/home/HomePage.js",["222","223","224","225"],"import React, { useState, useContext } from \"react\";\nimport {\n  Row,\n  Col,\n  Container,\n  Form,\n  Jumbotron,\n  FormControl,\n} from \"react-bootstrap\";\nimport NavbarComp from \"../../components/navbar/NavbarComp\";\nimport stylehome from \"./home.module.css\";\nimport CardComp from \"../../components/card/CardComp\";\nimport CardTrip from \"../../components/card/CardTrip\";\nimport { UserContext } from \"../../context/Contextusr\";\nimport { Link } from \"react-router-dom\";\nfunction HomePage() {\n  const [state, dispatch] = useContext(UserContext);\n  const trips = JSON.parse(localStorage.getItem(\"trip\"));\n  const [filterdata, setfilterdata] = useState([]);\n\n  const handleSearch = (e) => {\n    const search = e.target.value;\n    const newfilter = trips?.filter((value) => {\n      return value?.country.includes(search);\n    });\n    setfilterdata(newfilter);\n  };\n\n  return (\n    <>\n      <NavbarComp />\n      <Jumbotron className={stylehome.jmbtrn}>\n        <div className={stylehome.dv}>\n          <h1 className={stylehome.ttl}>Explore</h1>\n          <p className={stylehome.ttx}>Your Amazing City Together</p>\n        </div>\n        <Container>\n          <label\n            for=\"find\"\n            style={{\n              color: \"#fff\",\n              fontWeight: \"bold\",\n              textShadow: \"1px 1px rgb(71, 71, 71) \",\n            }}\n          >\n            find Great places Holiday\n          </label>\n          <Form className=\"d-flex\">\n            <br />\n            <FormControl\n              type=\"search\"\n              placeholder=\"Search\"\n              className=\"me-2\"\n              aria-label=\"Search\"\n              name=\"search\"\n              id=\"find\"\n              onChange={handleSearch}\n            />\n            <button variant=\"outline-success\" className={stylehome.bntnsrc}>\n              Search\n            </button>\n          </Form>\n          {!filterdata?.length ? (\n            <span></span>\n          ) : (\n            filterdata.map((dat, i) => (\n              <div className={stylehome.srch} key={i}>\n                <Link className={stylehome.lnk} to={\"/detail/\" + dat?.id}>\n                  {dat?.title}\n                </Link>\n              </div>\n            ))\n          )}\n        </Container>\n      </Jumbotron>\n      <Container>\n        <CardComp />\n      </Container>\n      <h2 className={stylehome.sc}>Group Tour</h2>\n      <br />\n      <Container>\n        <CardTrip />\n      </Container>\n\n      <br />\n    </>\n  );\n}\n\nexport default HomePage;\n","/home/usermint/Desktop/dewetour/src/components/navbar/NavbarComp.js",["226","227","228","229","230","231"],"import React, { useState, useContext } from \"react\";\nimport { Navbar, Container, Dropdown } from \"react-bootstrap\";\nimport log from \"../../assets/icons/Icon.png\";\nimport { Link, useHistory } from \"react-router-dom\";\nimport \"./navstyle.css\";\nimport LoginMod from \"../modals/LoginMod\";\nimport RegisterMod from \"../modals/RegisterMod\";\nimport { UserContext } from \"../../context/Contextusr\";\nimport icusr fr